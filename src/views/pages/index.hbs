<div>
    <div class="row no-gutters">
        <div class="col-12 text-center">
            <main>
              <div class="header-index">
                  <img src="/img/rotarywhite.png" width="50px">
                  <h1>Club Rotario La Paz</h1>
              </div> 
                <div class="container">
                <div class="row">
                  {{#each posts}}

                    <div class="col-sm-6">
                      <div class="card">
                        <img class="card-img-top" src="/profiles/{{this.profile}}">
                        <div class="card-body">
                          <h5 class="card-title">{{this.title}}</h5>
                          <p class="card-text">{{this.description}}</p>
                          <a href="/post/{{this.post_id}}" class="btn btn-primary btn-block">Ver publicacion</a>
                        </div>
                        <div class="card-footer"><small class="text-lead">Publicado el {{dateformat this.published_at}}</small></div>
                      </div>
                    </div>
                  {{/each}}
                    <div class="col-sm-6">
                      <div class="card bg-primary">
                        <div class="card-body">
                          <h5 class="card-title">Agenda</h5>
                          <p class="card-text"></p>
                        </div>
                      </div>
                    </div>
                  </div>
              </div> 
            </main>
          </div>
    </div>
</div>