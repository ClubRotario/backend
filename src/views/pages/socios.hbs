<header class="masthead history">
    <div class="container">
        <div class="masthead-subheading">Club Rotario La Paz</div>
        <div class="masthead-heading text-uppercase">Nuestros Socios</div>
    </div>
</header>
<section class="page-section bg-light" id="team">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Miembros</h2>
            <h3 class="section-subheading text-muted">Equipo activo de club rotario La Paz</h3>
        </div>
        <div class="row">
            {{#each members }}                
                <div class="col-lg-4">
                    <div class="team-member">
                        <h4><i class="fas fa-user text-primary mr-2"></i>{{this.fullname}}</h4>
                        <p class="text-muted text-uppercase">{{this.charge}}</p>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
</section>